{
  "openapi": "3.0.1",
  "info": {
    "title": "MentorshipHub.Api",
    "version": "1.0"
  },
  "paths": {
    "/api/UploadTask": {
      "post": {
        "tags": [
          "UploadTask"
        ],
        "operationId": "UploadTask",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/UploadDto"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadDto"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/UploadDto"
                }
              }
            }
          }
        }
      }
    },
    "/api/UserAuthentication/login": {
      "post": {
        "tags": [
          "UserAuthentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/UserAuthentication/register": {
      "post": {
        "tags": [
          "UserAuthentication"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "Email",
                  "FirstName",
                  "LastName",
                  "Password",
                  "Username"
                ],
                "type": "object",
                "properties": {
                  "ProfilePicture": {
                    "type": "string",
                    "format": "binary"
                  },
                  "FirstName": {
                    "maxLength": 30,
                    "minLength": 0,
                    "type": "string"
                  },
                  "LastName": {
                    "maxLength": 30,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Email": {
                    "maxLength": 60,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Username": {
                    "maxLength": 30,
                    "minLength": 0,
                    "type": "string"
                  },
                  "Password": {
                    "maxLength": 60,
                    "minLength": 0,
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "ProfilePicture": {
                  "style": "form"
                },
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Username": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginModel": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UploadDto": {
        "type": "object",
        "properties": {
          "isSuccess": {
            "type": "boolean"
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}